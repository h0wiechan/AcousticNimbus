<style>
@import url("https://fonts.googleapis.com/css?family=Nunito");
</style>
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

<% if logged_in? %>
  <script type="text/javascript">
    window.currentUser = <%= render(
      "api/users/user.json.jbuilder",
      user: current_user
    ).html_safe %>
  </script>
<% end %>


<main id="root"></main>

<script type="text/javascript">
  window.logo = "<%= asset_path "an_logo.png" %>"
  window.barLogo = "<%= asset_path "bar_logo.png" %>"
  window.default_avatar = "<%= asset_path "default_avatar.png" %>"
  window.play_button = "<%= asset_path "play_button.png" %>"
  window.pause_button = "<%= asset_path "pause_button.png" %>"
  window.dogs = "<%= asset_path "dogs.jpg" %>"
</script>
